<%- include('bootheader') -%>
<%- include('menubar') -%>
<div class="p-3 mb-4 bg-light">
  <div class="container-fluid ">
    <h1 class="pb-2 border-bottom">
      Edit Skill </h1>
    <div class="p-2">

      <form method="post" action="/editSupportFiles/<%= courseId %>">
        Preamble:
        <hr>
        <textarea name="preamble" cols="120"><%= preamble['text'] %></textarea>
        <hr>
        Title:
        <hr>
        <textarea name="title" cols="120"><%= title['text'] %></textarea>
        <hr>
        <input class="btn btn-lg btn-primary" type="submit">
      </form>
    </div>
  </div>
</div>

